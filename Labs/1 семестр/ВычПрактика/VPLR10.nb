 RowBox[{
  RowBox[{"f", "[", "list_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"k", ",", "l1", ",", "l0", ",", "s"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"k", "=", 
      RowBox[{"MinimalBy", "[", 
       RowBox[{"list", ",", "First"}], "]"}]}], ";", "\[IndentingNewLine]", 
